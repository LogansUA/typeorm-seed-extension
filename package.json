{
  "name": "typeorm-seed-extension",
  "version": "0.0.3",
  "description": "TypeORM seed extensions",
  "main": "cli.js",
  "scripts": {
    "build": "yarn lint && yarn clean && tsc && cp package.json ./lib/package.json && cp README.md ./lib/README.md",
    "lint": "tslint -c ./tslint.json ./src/**/*.ts --format stylish",
    "clean": "rm -rf ./lib"
  },
  "bin": {
    "typeorm-seed": "./lib/cli.js"
  },
  "engines": {
    "node": ">=8.11.3"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "commander": "^2.18.0",
    "faker": "^4.1.0",
    "glob": "^7.1.3",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/faker": "^4.1.3",
    "@types/glob": "^5.0.35",
    "@types/node": "^10.5.2",
    "@types/reflect-metadata": "0.1.0",
    "reflect-metadata": "^0.1.12",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typescript": "2.8.3"
  },
  "peerDependencies": {
    "typeorm": "~0.2"
  }
}
